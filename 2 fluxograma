#include <stdio.h>
int main(){
    char carro [50];
    int velocidade;
    
    printf("digite o nome do carro");
    scanf("%s",carro);
    
    printf(" digite a velocidade do carro");
    scanf("%d", &velocidade);
    
    if(velocidade>=18){
        printf("rapido");
        
    }else{
    printf("devagar");
    }
     return 0;
}
